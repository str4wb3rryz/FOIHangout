<Window x:Class="FOIHangout_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FOIHangout_WPF"
        mc:Ignorable="d"
        Title="FOIHangout" Height="450" Width="800" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Loaded="Window_Loaded" Background="#FFEFEFEF" KeyDown="Window_KeyDown">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Stretch">
        <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center" Background="White">
            <ContentControl x:Name="contentControl"/>
        </DockPanel>
    </StackPanel>

</Window>
